{
  "name": "fastcomments-vue",
  "version": "2.2.0",
  "author": "winrid",
  "license": "MIT",
  "repository": "fastcomments/fastcomments-vue",
  "description": "A Vue library for FastComments",
  "main": "dist/fastcomments-vue.ssr.js",
  "browser": "dist/fastcomments-vue.esm.js",
  "module": "dist/fastcomments-vue.esm.js",
  "unpkg": "dist/fastcomments-vue.min.js",
  "types": "fastcomments-vue.d.ts",
  "files": [
    "dist/*",
    "fastcomments-vue.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "serve-toggle-dark-mode": "vue-cli-service serve dev/serve-dark-mode-toggle.ts",
    "serve-pagination": "vue-cli-service serve dev/serve-pagination.ts",
    "serve-collab-chat": "vue-cli-service serve dev/serve-collab-chat.ts",
    "serve-image-chat": "vue-cli-service serve dev/serve-image-chat.ts",
    "serve-live-chat": "vue-cli-service serve dev/serve-live-chat.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "fastcomments-typescript": "^1.6.21",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "typescript": "^4.7.3",
    "vue": "^2.6.11",
    "vue-router": "^3.4.8",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
